                </div>

                </div>
           

                <div class="col-10">

                    <div class="home-searchbar">
                        <!-- <i class="bi bi-search"></i> -->
                        <form action="/notes" method="POST">
                           <input type="text" name="title" placeholder="Take a note" style="width: 550px; display: none;" >
                        </form>
                    </div>

                    <div class="home-searchbar2">

                      <div class="input1" contenteditable="true" data-placeholder="(untitled)"  aria-multiline="true"
                        role="textbox" tabindex="0" aria-label="Title" spellcheck="true">
                      </div>

                      <div class="input2" contenteditable="true" data-placeholder="(untitled)"  aria-multiline="true"
                        role="textbox" tabindex="0" aria-label="Title" spellcheck="true">
                      </div>

                    </div>

                    <!-- <div class="home-box-container"> -->
                    <div class="home-group">
                      <% note.forEach(function(note, index) { %>
                
                        <div class="border col-4">
                            <%= note.title %> <br>
                            <%= note.description %> 

                            <div class="icon">
                                <i class="bi bi-bell"></i>
                                <i class="bi bi-person-plus-fill"></i>
                                <i class="bi bi-palette"></i>
                                <i class="bi bi-file-image-fill"></i>
                                <i class="bi bi-file-arrow-down"></i>
                                <div class="dropdown ">
                                    <a type="button" class="" data-bs-toggle="dropdown">
                                        <svg viewBox="0 0 24 24" width="24" height="24" class="">
                                            <path fill="currentColor" d="M12 7a2 2 0 1 0-.001-4.001A2 2 0 0 0 12 7zm0 2a2 2 0 1 0-.001 3.999A2 2 0 0 0 12 9zm0 6a2 2 0 1 0-.001 3.999A2 2 0 0 0 12 15z">
                                            </path>
                                        </svg>
                                    </a>
                                  
                                    <ul class="dropdown-menu">
                                        <li><a href="/notes/<%= note.title %> " type="button" class="dropdown-item" onclick="myDelete()">delete</a></li> 
                                        <li><a class="dropdown-item" href="#">Link 3</a></li>
                                    </ul>
                                  </div>
                                
                            </div>
                        </div>
                 <% }) %>
                      
                    </div>
        
                </div>
            </div>



        </div>

    </section>


    
    

















